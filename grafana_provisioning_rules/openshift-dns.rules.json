                {
                    "uid": "ee651158-0cb7-5498-a99c-22e6c7a9d39f",
                    "title": "CoreDNSPanicking",
                    "condition": "threshold",
                    "data": [
                        {
                            "refId": "query",
                            "queryType": "prometheus",
                            "relativeTimeRange": {
                                "from": 660,
                                "to": 60
                            },
                            "datasourceUid": "REPLACE_ME_WITH_YOUR_DATASOURCE",
                            "model": {
                                "datasource": {
                                    "type": "prometheus",
                                    "uid": "REPLACE_ME_WITH_YOUR_DATASOURCE"
                                },
                                "expr": "increase(coredns_panics_total[10m]) \u003e 0",
                                "instant": true,
                                "intervalMs": 1000,
                                "maxDataPoints": 43200,
                                "range": false,
                                "refId": "query"
                            }
                        },
                        {
                            "refId": "prometheus_math",
                            "queryType": "math",
                            "relativeTimeRange": {
                                "from": 0,
                                "to": 0
                            },
