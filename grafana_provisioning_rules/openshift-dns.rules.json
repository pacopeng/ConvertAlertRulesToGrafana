                                "expression": "is_number($query) || is_nan($query) || is_inf($query)",
                                "intervalMs": 1000,
                                "maxDataPoints": 43200,
                                "refId": "prometheus_math",
                                "type": "math"
                            }
                        },
                        {
                            "refId": "threshold",
                            "queryType": "threshold",
